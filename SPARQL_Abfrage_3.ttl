### Die Gruppe möchte dorthin fahren, benötigt aber Informationen über die Abfahrts- ###
### und Ankunftszeiten der einzelnen Busreisezentrum ###
PREFIX schema: <https://schema.org/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX trip: <https://bmake.th-brandenburg.de/Trip#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX sc: <http://purl.org/science/owl/sciencecommons/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX ec: <http://eulergui.sourceforge.net/contacts.owl.n3#>

SELECT ?trip  ?provider ?departureTime ?arrivalTime 

WHERE {
 ?trip a schema:BusTrip;
        schema:departureTime ?departureTime;
        schema:arrivalTime ?arrivalTime;
        schema:provider ?provider.
}
  
LIMIT 25